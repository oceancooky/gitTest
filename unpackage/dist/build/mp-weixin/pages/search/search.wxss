.zone_title {
	width: 100%;
	height: 100rpx;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
	position: relative;
	background-color: #f4f4f4;
}
.zone_title image {
	height: 100%;
	width: 174rpx;
}
.search-title {
	font-size: 26rpx;
	color: rgba(51, 51, 51, 1);
	clear: both;
}
.search-item {
	height: 50rpx;
	line-height: 50rpx;
	border-radius: 50rpx;
	padding: 0 30rpx;
	font-size: 22rpx;
	background-color: #e2e2e2;
	color: rgba(51, 51, 51, 1);
	margin-right: 20rpx;
	float: left;
}
.noCache {
	font-size: 24rpx;
	color: #999;
	margin: 10rpx 0 0 30rpx;
}
.search-placeholder {
	overflow: hidden;
}
.delete-btn {
	background-image: url('data:image/png;base64,');
}

