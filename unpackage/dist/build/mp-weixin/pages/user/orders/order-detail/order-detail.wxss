
page {
	background-color: #F8F8F8;
}
.goods_card {
	height: 150rpx;
}
.goods_card_img {
	width: 150rpx;
	height: 150rpx;
}
.content {
	background-color: #FFFFFF;
}
.order-state {
	height: 160rpx;
	background: #FFFFFF;
	color: #FFFFFF;
	-webkit-box-align: start;
	-webkit-align-items: flex-start;
	    -ms-flex-align: start;
	        align-items: flex-start;
	padding-left: 108rpx;
}
.order-state-title {
	font-size: 32rpx;
	font-weight: 500;
}
.order-state-subtitle {
	font-size: 24rpx;
	font-weight: 400;
}
.address {
	height: 174rpx;
	background-color: #FFFFFF;
}
.address-info {
	-webkit-box-flex: 2;
	-webkit-flex: 2;
	    -ms-flex: 2;
	        flex: 2;
}
.address-name {
	ont-size: 30rpx;
	font-weight: bold;
	color: rgba(34, 34, 34, 1);
}
.address-phone {
	font-size: 28rpx;
	font-weight: 400;
	color: rgba(153, 153, 153, 1);
	opacity: 0.9;
	margin-left: 32rpx;
}
.address-detail {
	font-size: 26rpx;
	font-weight: 400;
	color: rgba(34, 34, 34, 1);
	margin-top: 20rpx;
}
.address-edit {
	width: 78rpx;
	height: 88rpx;
	background-image: url('data:image/png;base64,');
	background-size: 41rpx 40rpx;
	background-position: left center;
	background-repeat: no-repeat;
}
.order-item-goods {
	padding: 20rpx 0;
	border-top: none;
}
.orders-goods-wrap .order-item-goods:last-child {
	border-bottom: none;
}
.menu-title {
	font-size: 28rpx
}
.info-item {
	font-size: 26rpx;
	font-weight: 400;
	color: rgba(102, 102, 102, 1);
	line-height: 50rpx;
}
.info-item view {
	margin-left: 20rpx;
}
.remark {
	border-bottom: none;
	border-top: solid 1rpx #e9e9e9;
	-webkit-box-pack: start;
	-webkit-justify-content: flex-start;
	    -ms-flex-pack: start;
	        justify-content: flex-start;
}
.remark view {
	-webkit-box-flex: 2;
	-webkit-flex: 2;
	    -ms-flex: 2;
	        flex: 2;
	margin-left: 20rpx;
	font-weight: 400;
	color: #666666;
}
.goods-total {
	font-size: 24rpx;
	font-family: PingFangSC-Regular;
	font-weight: 400;
	color: rgba(102, 102, 102, 1);
	border-bottom: none;
	border-top: solid 1rpx #e9e9e9;
}
.goods-total view {
	font-size: 30rpx;
	font-weight: 500;
	color: rgba(203, 14, 14, 1);
	padding-right: 0;
	text-align: right;
	line-height: 30rpx;
	width: 550rpx;
}
.order-item-btns {
	-webkit-box-pack: end;
	-webkit-justify-content: flex-end;
	    -ms-flex-pack: end;
	        justify-content: flex-end;
	height: 100rpx;
	border-top: solid 1rpx #e9e9e9;
}
.order-item-btns button {
	margin-right: 0;
	margin-left: 20rpx;
	padding: 0 40rpx;
}
.contact {
	height: 110rpx;
	font-size: 28rpx;
	font-weight: bold;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
	color: #4483eb;
	border-top: solid 1rpx #e9e9e9;
	position: relative;
}
.contact image {
	width: 35rpx;
	height: auto;
	margin-right: 15rpx;
}
.contact button {
	position: absolute;
	width: 100%;
	height: 100%;
	opacity: 0;
	z-index: 10;
	left: 0;
	top: 0;
}
.goods_card_info {
	padding: 6rpx 0;
}

