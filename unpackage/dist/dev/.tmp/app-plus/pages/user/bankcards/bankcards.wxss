
.bank-card {
	border-radius: 6rpx;
	height: 120rpx;
	margin-top: 30rpx;
	padding: 30rpx;
	-webkit-box-shadow: 0rpx 4rpx 14rpx 0rpx rgba(102, 102, 102, 0.29);
	        box-shadow: 0rpx 4rpx 14rpx 0rpx rgba(102, 102, 102, 0.29);
}
.cards-list .bank-card:nth-of-type(even) {
	background-image: url('data:image/png;base64,');
	background-size: 100% 100%;
}
.cards-list .bank-card:nth-of-type(odd) {
	background-image: url('data:image/png;base64,');
	background-size: 100% 100%;
}
.cards-list .bank-card:last-child {
	background-image: none;
	text-align: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
	font-size: 30rpx;
	font-weight: 500;
	color: rgba(181, 182, 183, 1);
	border: 5rpx dashed rgba(230, 230, 230, 1);
}
.cards-list .bank-card:last-child image {
	width: 24rpx;
	height: auto;
	margin-right: 12rpx;
}
.bank-name {
	font-size: 28rpx;
	font-weight: 500;
	color: rgba(255, 255, 255, 1);
}
.bank-card-number {
	font-size: 30rpx;
	font-weight: 400;
	color: rgba(236, 236, 236, 1);
	margin-top: 14rpx;
}
.delete-btn {
	width: 44rpx;
	height: 44rpx;
	background-image: url('data:image/png;base64,');
	background-size: 44rpx 44rpx;
	float: right;
}

