
page {
	padding-bottom: 120rpx;
}
.content {
	border-top: solid 1rpx #d7d7d7;
}
.address {
	padding: 30rpx 30rpx;
	border-bottom: solid 1rpx #d7d7d7;
}
.address-info {
	width: 100%;
}
.address-def {
	width: 60rpx;
	height: 30rpx;
	background: rgba(235, 42, 56, 1);
	border-radius: 15rpx;
	font-size: 20rpx;
	font-family: PingFangSC-Medium;
	font-weight: 500;
	color: rgba(255, 255, 255, 1);
	line-height: 30rpx;
	text-align: center;
	margin-right: 10rpx;
}
.address-name {
	ont-size: 30rpx;
	font-weight: bold;
	color: rgba(34, 34, 34, 1);
}
.address-phone {
	font-size: 28rpx;
	font-weight: 400;
	color: rgba(153, 153, 153, 1);
	opacity: 0.9;
	margin-left: 32rpx;
}
.address-detail {
	font-size: 26rpx;
	font-weight: 400;
	color: rgba(34, 34, 34, 1);
	margin-top: 20rpx;
	/* align-items: flex-start */
}
.address-edit {
	width: 60rpx;
	height: 60rpx;
	background-image: url('data:image/png;base64,');
	background-size: 40rpx 40rpx;
	background-position: right center;
	background-repeat: no-repeat;
	margin-left: 30rpx;
}

